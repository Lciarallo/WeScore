<main class="flex_main">
	<section class="sectionCamp">
		<div class="cardDashboard">
			<p class="cardDashboard_title">Campeonatos <a href="/painelws/campeonato/add"><i class="ri-add-fill"></i>{{!-- <i class="ri-add-circle-line"></i> --}}</a></p>
			<div class="container__">
				<div class="cardDashboard_container">

					{{#each campeonatos}}
						<div class="cardDashboard_division">
							<div class="cardDashboard_division_image">
								<img
									src="http://localhost:3001/public/upload/img/campeonato/{{logoCampeonato}}"
									alt="logo do campeonato"
								/></div>
							<div class="cardDashboard_division_content">
								<div class="cardDashboard_division_text">
									<span
										class="cardDashboard_division_name"
									>{{nomeCampeonato}}</span>
									<p class="cardDashboard_division_username">
										{{Usuario.nomeUsuario}}</p>
								</div>

								<div class="paste-button">
									<button class="painelws_Btn">
										<i class="ri-more-2-fill"></i>
										<div class="painelws_Btn_text">Opções</div>
									</button>
									<div class="dropdown-content">
										<a id="top" href="/painelws/campeonatos/{{idCampeonato}}"><i class="ri-pencil-line"></i>Editar</a>
										{{! <a id="middle" href="#"></a> }}
										<a id="bottom" href="#"><i class="ri-delete-bin-line"></i></i>Remover</a>
									</div>
								</div>

							</div>
						</div>
					{{/each}}

				</div>
			</div>
			<a class="cardDashboard_more" href="#">See more</a>
		</div>
		<div class="cardDashboard">
			<p class="cardDashboard_title">Partidas {{campeonatos.idCampeonato}} <a href="/painelws/partida/add"><i class="ri-add-fill"></i>{{!-- <i class="ri-add-circle-line"></i> --}}</a></p>
			<div class="container__">
				<div class="cardDashboard_container">
					{{#each partidas}}
						<div class="cardDashboard_division">
							<div class="cardDashboard_division_image"></div>
							<div class="cardDashboard_division_content">
								<div class="cardDashboard_division_text">
									<span class="cardDashboard_division_name">Partida:
										{{idPartida}}</span>
									<p class="cardDashboard_division_username">{{nomeTime1}}
										vs
										{{nomeTime2}}</p>
								</div>
								<button class="painelws_Btn">
										<i class="ri-more-2-fill"></i>
										<div class="painelws_Btn_text">Opções</div>
									</button>
							</div>
						</div>
					{{/each}}

				</div>
			</div>
			<a class="cardDashboard_more" href="#">See more</a>
		</div>
	</section>
	<section2 class="sectionCamp2">
		<div class="cardDashboard2">
			<p class="cardDashboard_title">Times <a href="/painelws/time/add"><i class="ri-add-fill"></i>{{!-- <i class="ri-add-circle-line"></i> --}}</a></p>
			<div class="container__">
				<div class="cardDashboard_container">
					{{#each times}}
						<div class="cardDashboard_division">
							<div class="cardDashboard_division_image">
								<img
									src="http://localhost:3001/public/upload/img/time/{{logoTime}}"
									alt="logo do time"
								/>
							</div>
							<div class="cardDashboard_division_content">
								<div class="cardDashboard_division_text">
									<span class="cardDashboard_division_name">{{nomeTime}} </span>
									<p class="cardDashboard_division_username">ID time:
										{{idTime}}</p>
								</div>
								<button class="painelws_Btn">
										<i class="ri-more-2-fill"></i>
										<div class="painelws_Btn_text">Opções</div>
									</button>
							</div>
						</div>
					{{/each}}

				</div>
			</div>
			<a class="cardDashboard_more" href="#">See more</a>
		</div>
	</section2>
	<section2 class="sectionCamp2">
		<div class="cardDashboard2">
			<p class="cardDashboard_title">Jogadores <a href="/painelws/jogador/add"><i class="ri-add-fill"></i>{{!-- <i class="ri-add-circle-line"></i> --}}</a></p>
			<div class="container__">
				<div class="cardDashboard_container">
					{{#each jogadores}}
						<div class="cardDashboard_division">
							<div class="cardDashboard_division_content">
								<div class="cardDashboard_division_text">
									<span class="cardDashboard_division_name">{{nomeJogador}}
										{{sobrenome}}</span>
									<p class="cardDashboard_division_username">
										Time:
										{{Time.nomeTime}}</p>
									<p class="cardDashboard_division_username">
										Nº Camiseta:
										{{numeroCamiseta}}</p>
								</div>
								<button class="painelws_Btn">
										<i class="ri-more-2-fill"></i>
										<div class="painelws_Btn_text">Opções</div>
									</button>
							</div>
						</div>
					{{/each}}

				</div>
			</div>
			<a class="cardDashboard_more" href="#">See more</a>
		</div>
	</section2>
</main>